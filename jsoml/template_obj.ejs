<div id="<%= obj.title.match(/[a-zA-Z0-9]+/g).join('') %>" class="object">

    <p class="description">
        <%= obj.description %>
    </p>

    <dl class="propertyList">
    <%
    let keys = Object.keys(obj.properties);
    for (let i = 0; i < keys.length; i++) {%>
        <dt class="property"><%= keys[i] %></dt>
        <dd>
            <dl class="propertyInfo">
                <dt class="propertyInfoLabel">Type:</dt>
                <dd class="propertyInfoItem"><%= obj.properties[keys[i]].type %></dd>
            </dl>
        </dd>
    <% } %>
    </dl>
</div>
